[
    {   "name": "ohNet",
        "type": "openhome",
        "platform-specific": true,
        "version": "1.2.1581",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}",
        "configure-args": [
            "--ohnet-lib-dir", "${main-dir}/lib",
            "--ohnet-include-dir", "${main-dir}/include/ohnet"
        ],
        "_comment": [
            "See ohos for more examples, and ohdevtools/dependencies.py for details",
            "of the predefined values and expansion rules."
        ]
    },
    {   "name": "ohWafHelpers",
        "type": "openhome",
        "platform-specific": false,
        "version": "0.0.77",
        "archive-filename": "${name}-${version}.tar.gz"
    },
    {   "name": "TestTones",
        "type": "external",
        "version": "1.9",
        "archive-path": "http://core.linn.co.uk/~artifacts/artifacts/testtones/TestTones-${version}.zip",
        "dest": "dependencies/AnyPlatform/TestTones/"
    },
    {   "name":             "testharness",
        "version":          "0.1.34",
        "type":             "openhome",
        "archive-filename": "${name}-${version}.tar.gz",
        "dest":             "dependencies/AnyPlatform/"
    },
    {   "name": "libplatform",
        "type": "internal",
        "version": "0.0.118",
        "archive-suffix": "-${debugmode}",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore": "${platform-ignore[$platform]}"
    },
    {   "name":             "libosa",
        "type":             "internal",
        "version":          "0.0.184",
        "archive-suffix":   "-Release",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}"
    },
    {   "name":             "transmute",
        "version":          "0.0.11",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}",
        "dest":             "dependencies/AnyPlatform/"
    },
    {   "name":             "volkano-tools",
        "version":          "0.0.4",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-armv5": false,
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}",
        "dest":             "dependencies/AnyPlatform/${name}/"
    },
    {   "name": "openssl",
        "type": "openhome",
        "platform-specific": true,
        "version": "0.2.55",
        "archive-filename": "${name}-${version}-${archive-platform}-${debugmode}.tar.bz2"
    },
    {   "name": "ohNetmon",
        "type": "openhome",
        "platform-specific": true,
        "version": "1.0.28",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}"
    },
    {
        "name": "FunctionalTest",
        "type": "internal",
        "version": "0.0.531",
        "platform-specific": false,
        "archive-path": "http://core.linn.co.uk/~artifacts/artifacts/FunctionalTest/FunctionalTest-${version}.zip",
        "dest": "dependencies/AnyPlatform/FunctionalTest"
    },
    {
        "name": "FunctionalTestData",
        "type": "internal",
        "version": "0.0.38",
        "platform-specific": false,
        "archive-path": "http://core.linn.co.uk/~artifacts/artifacts/FunctionalTestData/FunctionalTestData-${version}.zip",
        "dest": "dependencies/AnyPlatform/FunctionalTestData",
        "platform-ignore": {
            "Linux-ppc32": true,
            "Linux-armhf": true,
            "*": false },
        "ignore":           "${platform-ignore[$platform]}"
    }
]
