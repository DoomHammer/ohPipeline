[
    {   "name": "ohNet",
        "type": "openhome",
        "platform-specific": true,
        "version": "1.0.637",
        "archive-suffix": "-${debugmode}",
        "main-dir": "${dest}${name}-${platform}-${debugmode}",
        "configure-args": [
            "--ohnet-lib-dir", "${main-dir}/lib",
            "--ohnet-include-dir", "${main-dir}/include/ohnet"
        ],
        "_comment": [
            "See ohos for more examples, and ohdevtools/dependencies.py for details",
            "of the predefined values and expansion rules."
        ]
    },
    {   "name": "ohWafHelpers",
        "type": "openhome",
        "platform-specific": false,
        "version": "0.0.40",
        "archive-filename": "${name}-${version}.tar.gz"
    },
    {
        "name": "TestTones",
        "type": "external",
        "version": "1.4",
        "archive-path": "http://core.linn.co.uk/~artifacts/artifacts/testtones/TestTones-${version}.zip",
        "dest": "dependencies/AnyPlatform/TestTones/"
    },
    {   "name": "CherryPy",
        "type": "external",
        "version": "3.2.2",
        "archive-path": "http://core.linn.co.uk/~artifacts/artifacts/cherrypy/${name}-${version}.zip",
        "dest": "dependencies/AnyPlatform/CherryPy"
    },
    {   "name":             "testharness",
        "version":          "0.0.1",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "dest":             "dependencies/AnyPlatform/"
    },
    {   "name": "libplatform",
        "type": "internal",
        "version": "0.0.36",
        "archive-suffix": "-Release",
        "platform-ignore": {
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore": "${platform-ignore[$platform]}"
    },
    {   "name":             "libosa",
        "type":             "internal",
        "version":          "0.0.135",
        "archive-suffix":   "-Release",
        "platform-ignore": {
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}"
    },
    {   "name":             "transmute",
        "version":          "0.0.6",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}",
        "dest":             "dependencies/AnyPlatform/"
    },
    {   "name":             "volkano-tools",
        "version":          "0.0.4",
        "type":             "internal",
        "archive-filename": "${name}-${version}.tar.gz",
        "platform-ignore": {
            "Core-armv6": false,
            "Core-ppc32": false,
            "*": true },
        "ignore":           "${platform-ignore[$platform]}",
        "dest":             "dependencies/AnyPlatform/${name}/"
    }
]
